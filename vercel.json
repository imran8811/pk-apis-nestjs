{
    "version": 2,
    "name": "pk-be-nestjs",
    "buildCommand": "npm start",
    "installCommand": "npm install",
    "builds": [
      {
        "src": "dist/main.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/main.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
        "headers": {
            "Access-Control-Allow-Origin": "*"
        }
      }
    ]
  }